{
  "name": "hello-Books",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5100",
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "font-awesome": "^4.7.0",
    "jquery": "@>=3.0.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "popper.js": "@1.11.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^3.0.5",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.11"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/scss/styles.scss -o src/assets/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/scss/styles.scss -o src/assets/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-react-app": "^2.0.0",
    "webpack": "^3.5.5"
  }
}
