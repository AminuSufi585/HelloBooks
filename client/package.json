{
  "name": "hello-books",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "axios": "^0.16.2",
    "bootstrap": "4.0.0-alpha.6",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "jquery": "^3.0.0",
    "lodash": "^4.17.4",
    "mdbootstrap": "^4.4.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "popper.js": "^1.11.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.11",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "ripple": "^0.1.2",
    "shortid": "^2.2.8",
    "tether": "^1.4.0",
    "waves": "^0.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/scss -o src/assets/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/scss/styles.scss -o src/assets/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-react-app": "^2.0.0",
    "webpack": "^3.5.5"
  }
}
